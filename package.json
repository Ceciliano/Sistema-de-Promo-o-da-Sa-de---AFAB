  "scripts": {
    "dev": "node -r sucrase/register src/server.js",
    "build": "sucrase ./src -d ./dist --transforms imports",
    "dev-seed": "run-s unmigrate migrate seedall dev",
    "seedprop": "run-s unmigrate migrate seedall",
    "debug": "node -r sucrase/register --inspect src/server.js",
    "seed": "sequelize db:seed --seed 20191118154046-admin-user.js",
    "seedall": "sequelize db:seed:all",
    "migrate": "sequelize db:migrate",
    "unmigrate": "sequelize db:migrate:undo:all",
    "pretest": "NODE_ENV=test run-s unmigrate migrate seed",
    "test": "NODE_ENV=test jest -i --forceExit"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "^2.0.0",
