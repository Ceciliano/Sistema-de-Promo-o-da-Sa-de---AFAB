    "faker": "^5.1.0",
    "jest": "^27.4.7",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "ora": "^4.0.3",
    "prettier": "^1.19.1",
    "sequelize-cli": "^6.4.1",
    "sucrase": "^3.10.1",
    "supertest": "^4.0.2"
  },
